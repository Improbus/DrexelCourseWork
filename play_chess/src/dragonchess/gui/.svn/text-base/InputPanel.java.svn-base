package dragonchess.gui;

import javax.swing.JPanel;

import org.apache.log4j.Logger;

/**
 * The input JPanel.
 *
 * @author Group 4
 */
public class InputPanel extends JPanel {

	private static final long serialVersionUID = 334455667L;
	private static final Logger log = Logger.getLogger(InputPanel.class);

	/** */
	private DragonChessMW parent;

	/**
	 * Default constructor
	 */
	public InputPanel(DragonChessMW parent) {
		super();

		this.parent = parent;
		this.createInputPanel();

		return;
	}

	/**
	 *
	 */
	private void createInputPanel() {
		super.setBorder(null);

		return;
	}

}
