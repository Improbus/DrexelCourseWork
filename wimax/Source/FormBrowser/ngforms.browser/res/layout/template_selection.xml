<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="fill_parent"
             android:layout_height="fill_parent" android:background="@drawable/wallpaper">
             
    <ListView android:layout_width="fill_parent"
              android:id="@+id/contactList"
              android:layout_height="wrap_content"
              android:layout_weight="1"/>
     <LinearLayout
		android:id="@+id/linlayoutButtons"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_weight="0"
		>
		         
    
		<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" 
		android:text="             " 
		android:id="@+id/pageblank">
		</TextView>
		
    <Button android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:id="@+id/HomeButton" android:text="Back To Main Menu" android:drawableLeft="@drawable/home" android:background="@drawable/groundyellowbtn"/>
    
    	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" 
		android:text="             " 
		android:id="@+id/pageblanks" >
		</TextView>
		
	</LinearLayout>
	
</LinearLayout>

